#include "list.h"

/* functions definitions */

void createList(List *pl)
{
	pl->size = 0;
}


int listEmpty(List *pl)
{
	return !pl->size?0:1;
}


int listFull(List *pl)
{
	return pl->size == MAXLIST ? 1:0 ;
}


int listSize(List *pl)
{
	return pl->size;
}


